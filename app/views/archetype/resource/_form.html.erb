<%= simple_nested_form_for resourceful.resource.form_object do |f| %>
  <%= render partial: 'fieldset', collection: attributes.fieldsets(context), locals: {f: f, context: context} %>

  <div class="box">
    <div class="box-body clearfix">
      <div class='pull-right'>
        <%= link_to 'Back', resource_class, class: 'btn btn-sm btn-default' %>
        <%= f.submit 'Save', class: 'btn btn-sm btn-primary' %>
      </div>
    </div><!-- /.box-body -->
  </div><!-- /.box -->
<% end %>
